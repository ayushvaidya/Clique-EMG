
<!-- No tracks to display -->
<% if tracks.count == 0 %>
  <div class="no-post">
    <div class="no-post-img"></div>
      No tracks to show</br>
      <%= link_to "Explore Artists", explore_path, :class => "btn no-post-btn btn-primary secondary-background" %>
    </div>
    <% else %>
    <!-- Displays each track on the list -->
    <% tracks.each do |track| %>
    <%= render :partial => '/tracks/track', :locals => { :track => track } %>

 
  <% end %>
<% end %>












