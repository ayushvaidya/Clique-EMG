<div class="box panel panel-default">
  <div class="panel-Top">
    <div class="panel-Cover">
      <%= link_to (image_tag artist.cover_picture_url), artist, 'data-skip-pjax' => true %>
    </div>
    <div class="panel-Overlay">
    </div>



      <div class="panel-ProfilePic">
        <%= link_to (image_tag artist.profile_picture_url), artist, 'data-skip-pjax' => true %>
      </div>



    <div class="panel-Info">
      <!--<div class="panel-Btn">
        VIEW
      </div>-->
      <div class="panel-Info-Name">
        <%= link_to artist.name, artist, 'data-skip-pjax' => true   %>
      </div>
      <div class="panel-Info-Clique">
        <%= link_to artist.followers.count, artist, 'data-skip-pjax' => true %> Followers
      </div>
    </div>




  </div>
</div>